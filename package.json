{
	"name": "type-plugin",
	"displayName": "type-plugin",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:type-plugin.convertToArrowFunction",
		"onCommand:type-plugin.camel-case",
		"onCommand:type-plugin.convertVarToLet",
		"onCommand:type-plugin.convertClassNameFirstLetterToCaps",
		"onCommand:type-plugin.convertInterfaceNameFirstLetterToCaps"

	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "type-plugin.camel-case",
				"title": "Convert To CamelCase"
			},
			{
				"command": "type-plugin.convertToArrowFunction",
				"title": "Convert To Variable Declaration"
			},
			{
				"command": "type-plugin.convertVarToLet",
				"title": "Convert Var To Let"
			},
			{
				"command": "type-plugin.convertClassNameFirstLetterToCaps",
				"title": "Convert Class Name To Standard"
			},
			{
				"command": "type-plugin.convertInterfaceNameFirstLetterToCaps",
				"title": "Convert interface Name To Standard"
			}
		],
		"keybindings": [
			{
			  "command": "type-plugin.camel-case",
			  "key": "alt+shift+enter",
			  "when": "editorTextFocus"
			},
			{
				"command": "type-plugin.convertToArrowFunction",
				"key": "alt+enter",
				"when": "editorTextFocus"
			},
			{
				"command": "type-plugin.convertVarToLet",
				"key": "alt+c",
				"when": "editorTextFocus"
			}
		]	
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@babel/core": "^7.10.2",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@babel/generator": "^7.10.2",
		"@babel/parser": "^7.10.2",
		"@babel/traverse": "^7.10.1",
		"@babel/types": "^7.10.2",
		"@types/babel__generator": "^7.6.1",
		"@types/babel__traverse": "^7.0.12",
		"@types/lodash": "^4.14.155",
		"lodash": "^4.17.15"
	}
}
